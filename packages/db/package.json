{
  "name": "@resume-maker/db",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "drizzle-orm": "^0.44.7",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "next-auth": "5.0.0-beta.30",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "next-auth": "5.0.0-beta.30"
  }
}
