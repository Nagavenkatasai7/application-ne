{
  "name": "@resume-maker/ai",
  "version": "1.0.0",
  "description": "AI utilities for resume-maker zones (Anthropic, retry, JSON parsing)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./json-utils": {
      "types": "./dist/json-utils.d.ts",
      "import": "./dist/json-utils.js"
    },
    "./retry": {
      "types": "./dist/retry/index.d.ts",
      "import": "./dist/retry/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@resume-maker/types": "workspace:*",
    "uuid": "^11.0.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@types/node": "^20",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
